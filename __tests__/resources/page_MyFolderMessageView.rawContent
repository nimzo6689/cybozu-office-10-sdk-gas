<script type="text/javascript">
$(function() {
 var mentionEditorFrames = $('.mentionEditorFrame');

 // 通知一覧画面では複数のフォームに対応する必要があるためeachで回す
 mentionEditorFrames.each(function() {
 var baseElement = $(this);
 var mentionInputRow = baseElement.find('.mentionInputRow');
 var mentionButton = baseElement.find('.mentionButton');

 var form = baseElement.find('#Data-m85');
 var formWrapper = baseElement.find('.commentAreaWrapper');

 if (YAHOO.env.ua.ie === 6 || YAHOO.env.ua.ie === 7) {
 var leftMargin = parseInt(formWrapper.css('margin-left'));
 var rightMargin = parseInt(formWrapper.css('margin-right'));
 var textareaWidth = parseInt(form.width());
 textareaWidth += leftMargin + rightMargin;
 baseElement.width(textareaWidth + 'px');
 } else {
 form.css({
 width: '100%'
 });
 }

 var mentionInput = null;
 mentionInput = mentionInputRow.mentionInput().data('ui-mentionInput');

 var toggleMentionForm = function(disableFocus) {
 if (mentionButton.hasClass('mentionButtonSelected')) {
 mentionButton.removeClass('mentionButtonSelected');
 mentionInputRow.hide();
 } else {
 mentionButton.addClass('mentionButtonSelected');
 mentionInputRow.show();
 window.setTimeout(function() {
 if (!disableFocus) {
 baseElement.find('.cb-token-input-input').focus();
 }
 }, 0);
 }
 }

 mentionInputRow.off('mentioninputchange');
 mentionInputRow.on('mentioninputchange', function() {
 var count = mentionInput.countLabel();
 var countDiv = baseElement.find('.mentionLabelCountText');
 if (countDiv.length === 0) {
 mentionButton.append(countDiv);
 }
 if (count > 0) {
 countDiv.css({
 display: 'inline-block'
 });
 } else {
 countDiv.css({
 display: 'none'
 });
 }
 countDiv.html('：<span class="mentionLabelCount">' + count + '</span>人');
 });
 mentionButton.off('mousedown');
 mentionButton.on('mousedown', function(e) {
 mentionButton.trigger("toggleMentionForm");
 return false;
 });
 mentionButton.off('toggleMentionForm');
 mentionButton.on('toggleMentionForm', function(e, disableFocus) {
 toggleMentionForm(disableFocus);
 return false;
 });

 });
});
</script>
